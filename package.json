{
  "name": "thepersonaldev-wordpress-2020",
  "version": "1.0.0",
  "description": "Our first custom theme for 2020 ðŸŽ¨",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:front-end dev:blocks",
    "dev:front-end": "parcel assets/js/main.js assets/styles/main.scss assets/styles/editor.scss",
    "dev:blocks": "wp-scripts start assets/js/blocks/captioned-heading.js assets/js/blocks/labeled-progress-bar.js assets/js/blocks/projects-grid.js --output-path=dist/js/blocks",
    "build:front-end": "parcel build assets/js/main.js assets/styles/main.scss assets/styles/editor.scss",
    "build:blocks": "wp-scripts build assets/js/blocks/captioned-heading.js assets/js/blocks/labeled-progress-bar.js assets/js/blocks/projects-grid.js --output-path=dist/js/blocks",
    "build": "npm-run-all build:front-end build:blocks"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wordpress/scripts": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-dist": "0.0.6",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "@wordpress/components": "^9.3.0",
    "flexboxgrid": "^6.3.1",
    "normalize.css": "^8.0.1",
    "superfish": "^1.7.10"
  }
}
